{
  "name": "Lacework-Trigger",
  "type": "trigger",
  "execProgram": "node",
  "main": "app.js",
  "imgUrl": "lacework.png",
  "version": "1.0.0",
  "description": "Lacework Trigger",
  "methods": [
    {
      "name": "ALERT_WEBHOOK",
      "route": "post /webhook/lacework/alert",
      "viewName": "Alert from Lacework",
      "params": [
        {
          "name": "EVENT_TYPE",
          "viewName": "Event type",
          "type": "options",
          "options": [
            {
              "id": "Compliance",
              "name": "Compliance"
            }
          ]
        },
        {
          "name":"ID",
          "viewName":"Issue ID",
          "type": "string"
        }
      ]
    }
  ]
}